{
  "parser": "babel-eslint",
  "plugins": [
    "react"
  ],
  "extends": [
    "airbnb"
  ],
  "rules": {
    "no-underscore-dangle": ["error", { "allow": [ "_embedded" ] }],
    "no-alert": "warn",
    "jsx-a11y/no-static-element-interactions": "warn",
    "react/no-unused-prop-types": "warn",
    "react/no-did-mount-set-state": "warn",
    "react/no-danger": "warn",
    "no-param-reassign": "warn",
    "semi": [2, "never"],
    "no-undef": 0
  },
  "globals": {
    "confirm": true,
    "document": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "globalReturn": false,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  }
}
